{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(main)/(main1)/Game/PingPong/LocalGame/page.tsx", "app/(main)/(main1)/[Profile]/ProfileClient.tsx", "app/(main)/(main1)/[Profile]/page.tsx", "app/(main)/(main1)/pingpong/page.tsx"],
  "exclude": ["node_modules"]
}
